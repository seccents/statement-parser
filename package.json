{
	"name": "statement-parser",
	"version": "0.0.1",
	"description": "Package to parse Bank, Credit Card, Stock and Crypto Statements.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"lint": "eslint . --ext js,jsx,ts,tsx --fix",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com-personal:seccents/statement-parser.git"
	},
	"bugs": {
		"url": "https://github.com/elanchezhiyanr/dividend-parser/issues"
	},
	"homepage": "https://github.com/elanchezhiyanr/dividend-parser#readme",
	"keywords": [
		"Statement",
		"Parser",
		"Bank",
		"Credit Card",
		"Stock",
		"Crypto"
	],
	"author": "Elanchezhiyan Ravichandran",
	"license": "ISC",
	"dependencies": {
		"@fast-csv/parse": "^4.3.6",
		"moment": "^2.29.2"
	},
	"devDependencies": {
		"typescript": "^4.6.4",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^8.12.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "^2.6.2"
	},
	"lint-staged": {
		"**/*.{js,ts}": [
			"eslint . --ext js,jsx,ts,tsx --fix",
			"prettier --write ."
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"files": [
		"dist/**/*"
	]
}